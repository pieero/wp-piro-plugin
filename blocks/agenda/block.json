{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "piro-plugin/agenda",
	"version": "1.1.0",
	"title": "Mon Agenda",
	"category": "widgets",
	"icon": "editor-ul",
	"description": "Display futur and past events.",
	"supports": {
		"html": false,
		"color": {
            "text": true,
            "background": true,
            "link": true
        },
		"spacing": {
			"margin": true,  
			"padding": true, 
			"blockGap": true
		}
	},
	"textdomain": "piro-plugin",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"tags": {
			"type": "string",
			"default": ""
		},
		"categories": {
			"type": "string",
			"default": ""
		}
		,
		"nextTitle": {
			"type": "string",
			"default": "Prochainement"
		},
		"previousTitle": {
			"type": "string",
			"default": "Evénements passés"
		}

	},
	"example": {
		"attributes": {
			"preview": "true"
		}
	}
	
}